@model List<Vapers_Club.Models.ViewModels.asignaralmacen>
@{
    ViewBag.Title = "consultarprodal";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Productos por almacen</h2>
<br />
<br />
<div class="row">
    <div class="col-md-8">
        <table class="table">
            <thead>
                <tr>
                    <th>Producto</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var fila in Model)
                {
                    <tr>
                        <td>@fila.producto</td>
                        <td>
                            <input type="button" value="Eliminar" class="btn btn-danger" onclick="document.location.href='@Url.Action("eliminaraprodal", "Almacen", new { id = fila.idalmacen, prod = fila.producto })'" />
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <input type="button" value="Volver" class="btn btn-primary btn-block btn-lg" onclick="document.location.href='@Url.Content("~/Almacen/Index")'" />
    </div>
</div>